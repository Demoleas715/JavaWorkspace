package u6;

public class MultiplicationTable {

	public static void main(String[] args){
		for (int i = 1; i<=9; i++){
			System.out.printf("%5d", i);
		}
		System.out.println();
		System.out.println("   -------------------------------------------");
		for (int i = 1; i<=9; i++){
			System.out.printf("%5d|", i);
			
			
		}
	}
}
