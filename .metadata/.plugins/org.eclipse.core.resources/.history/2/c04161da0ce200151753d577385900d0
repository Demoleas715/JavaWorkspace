package u7;

import java.util.Random;

public class Sorting {

	public static int[] getNewArray (int size){
		Random rand = new Random();
		int[] nums = new int[size];
		for (int i = 0; i < size; i++)
			nums[i] = rand.nextInt(1000)
	}

